{
  "name": "test-React1",
  "version": "1.0.0",
  "description": "This repo was made to understand how to setup testing frameworks with React and also to understand how to execute different tests. As you can test React with jest and enzyme as well, this is therefore one way to do testing.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register --require ./test/helpers.js --require ./test/dom.js --recursive",
    "tdd": "npm test -- --watch",
    "dev": "webpack-dev-server --port 3000 --devtool eval --progress --colors --hot --content-base dist",
    "build": "webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thatdania/test-React1.git"
  },
  "keywords": [],
  "author": "Dania",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thatdania/test-React1/issues"
  },
  "homepage": "https://github.com/thatdania/test-React1#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "mocha": "^5.0.0",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "sinon": "^4.2.1"
  },
  "dependencies": {
    "jsdom": "^11.6.1"
  }
}
